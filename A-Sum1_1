public class TestSumm {
    public static void main(String[] args){
        Summa summa = new Summa();
        int summ = summa.summ();
        System.out.println("Сумма " +summ);
    }
}
class Summa {
    int i = 2;
    int j = 3;

    public int summ(){
        int summ1 = i + j;
        return summ1;
    }
}
